{
  "extends": [
    "eslint:recommended", 
    "plugin:@typescript-eslint/recommended",
    "next/babel",
    "next/core-web-vitals"
  ],
  "rules": {
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": ["warn"],
    "no-unused-expressions": "warn",
    "no-unused-labels": "warn",
    "indent": ["error", 4, { "SwitchCase": 1 }],
    "@next/next/no-img-element": "off"
  }
}